# .env 파일 (권장 형식)

# Gemini API Key (따옴표 제거)
GEMINI_API_KEY=YOURAPIKEY

# Firebase 설정 (JSON 문자열)
# JSON 문자열 자체는 값으로 인식되므로, 값 전체에 따옴표를 넣을 필요가 없습니다.
FIREBASE_CONFIG_JSON={"apiKey": "YOUR API", "appId": "YOUR APPID", "messagingSenderId": "YOUR messagingSenderId", "projectId": "yourprojectId"}

# 기타 설정 (일반 문자열)
APP_ID=basic-project-app

# INITIAL_AUTH_TOKEN (주석 처리 또는 빈 값 유지)
# INITIAL_AUTH_TOKEN=